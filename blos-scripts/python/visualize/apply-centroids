#!/usr/bin/python
import sys
#import matplotlib.pyplot as plt
import csv
import os
import fileinput
import math


in_centroids = sys.argv[1]
centroids = []

with open(in_centroids, 'rb') as file:
	for line in file:
		f=line.split(" ")
		centroids.append( ( float(f[0]), float(f[1]) ) )
for line in sys.stdin:
	fields=line.split(" ")
	x=float(fields[0])
	y=float(fields[1])
	c=0
	dmin=float("inf")
		
	for i,e in enumerate(centroids):
		d=math.sqrt( (x-e[0])*(x-e[0]) + (y-e[1])*(y-e[1]) )
		if d < dmin:
			c=i
			dmin=d

		
	print(str(c)+" "+str(x)+" "+str(y))
